library(sf)
library(elevatr)
library(raster)
library(rayshader)
library(dplyr)
# coordinates
shenandoah <- data.frame(x = c(-78.6823, -78.1622), y = c(38.3148, 38.8761))
elev <- get_elev_raster(shenandoah, z = 13, clip = 'bbox',
prj = "+proj=longlat +datum=WGS84 +ellps=WGS84 +towgs84=0,0,0")
elev <- get_elev_raster(shenandoah, z = 11, clip = 'bbox',
prj = "+proj=longlat +datum=WGS84 +ellps=WGS84 +towgs84=0,0,0")
elmat <- raster_to_matrix(elev)
elmat %>%
sphere_shade(texture = create_texture('#F27A7A', '#000000', '#7A472E', '#1A6408', '#aed9e0')) %>%
